@model IEnumerable<StudentsMVC.Models.Course>
@{
    ViewBag.Title = "GetCourseData";
}


<h2>Course Details</h2>
<table style="font-family: Arial" border="2">
    <tr>
        <th>Course Id</th>
        <th>Course Name</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.CourseId</td>
        <td>@item.CourseName</td>
    </tr>
    }
</table>

